{{#each users}}
    <div class="box box-link-hover-shadow" >
        <h2 class="name">{{username}}</h2>

        <a  href="/user/{{_id}}" class="button">
            <i class="fas fa-folder-open"></i>
        </a>

        <a href="/deleteUser/{{_id}}" class="button">
            <i class="fas fa-trash"></i>
        </a>
    </div>
{{/each}}