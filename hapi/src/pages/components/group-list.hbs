{{#each groups}}
    <div class="box box-link-hover-shadow" >
        <h2 class="name">{{title}}</h2>

        <a  href="/group/{{_id}}" class="button">
            <i class="fas fa-folder-open"></i>
        </a>

        <a href="/dashboard/deletegroup/{{_id}}" class="button">
            <i class="fas fa-trash"></i>
        </a>


    </div>
{{/each}}